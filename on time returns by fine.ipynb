  {
   "cell_type": "code",
   "execution_count": 9,
   "metadata": {},
   "outputs": [],
   "source": [
    "config = configparser.ConfigParser()\n",
    "'''\n",
    "config file should look like this\n",
    "[db]\n,
    "    connection_string = dbname='iii' user='' host='' password='' port=\n",
    "'''\n",
    "\n",
    "config.read('config_file.ini')\n",
    "\n",
    "try:\n",
    "    query = \"\"\"SELECT\n",
    "            t.loanrule_code_num,\n",
    "            t.itype_code_num,\n",
    "            COUNT(t.item_record_id) AS total_returns,\n",
    "            COUNT(t.item_record_id) FILTER(WHERE t.transaction_gmt::DATE <= t.due_date_gmt::DATE) AS total_ontime,\n",
    "            COUNT(t.item_record_id) FILTER(WHERE t.transaction_gmt::DATE > t.due_date_gmt::DATE) AS total_late,\n",
    "            ROUND(100 * (COUNT(t.item_record_id) FILTER(WHERE t.transaction_gmt::DATE > t.due_date_gmt::DATE) * 1.0) / COUNT(t.item_record_id),4)||'%' AS pct_late\n",
    "\n",
    "            FROM\n",
    "            sierra_view.circ_trans t\n",
    "            WHERE\n",
    "            t.op_code = 'i'\n",
    "\n",
    "            GROUP BY 1,2\n",
    "            ORDER BY 1,2\n",
    "                \"\"\"\n",
    "    #variable connection string should be defined in the imported config file\n",
    "    conn = psycopg2.connect( config['db']['connection_string'] )\n",
    "except:\n",
    "    print(\"unable to connect to the database\")\n",
    "    clear_connection()\n",
    "cursor = conn.cursor()\n",
    "cursor.execute(query)\n",
    "#For now, just storing the data in a variable. We'll use it later.\n",
    "sierra_rows = cursor.fetchall()\n",
    "conn.close()"
   ]
  },
